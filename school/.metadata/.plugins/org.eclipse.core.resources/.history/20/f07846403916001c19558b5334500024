import java.util.Scanner;

public class Test1 {

	public static void main(String[] args) {
		int box;
		int left;
		
		Scanner sc = new Scanner(System.in);
		System.out.println("오렌지의 개수를 입력하시오: ");
		
		box = sc.nextInt() / 10;
		left = sc.nextInt() % 10;
		
		System.out.println(box + "박스가 필요하고 " + left + "개가 남습니다.");
	}

}
