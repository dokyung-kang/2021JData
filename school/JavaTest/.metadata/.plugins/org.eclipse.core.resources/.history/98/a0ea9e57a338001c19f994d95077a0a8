import java.util.*;
public class AA {

	public static void main(String[] args) {
		int x = 0;
		boolean b;
		
		System.out.println(x++);
		System.out.println((int)3.5 + 3);
		System.out.println("%06.2f",(double)1 / 2);
		System.out.println(true && false || true);
		System.out.println(true && false && false || true);
		

	}

}
