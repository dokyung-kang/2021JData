
public class ShapeTest {

	public static void main(String[] args) {
		Movable r = new Rectangle();
		Movable t = new Triangle();
		Movable c = new Circle();
		Shape rr = new Rectangle();
		Shape tt = new Triangle();
		Shape cc = new Circle();
		
		int x;
		int y;
		
		x = (int)(Math.random() * 100) + 1;
		y = (int)(Math.random() * 100) + 1;
		rr.draw();
		r.move(x, y);
		x = (int)(Math.random() * 100) + 1;
		y = (int)(Math.random() * 100) + 1;
		tt.draw();
		t.move(x, y);
		x = (int)(Math.random() * 100) + 1;
		y = (int)(Math.random() * 100) + 1;
		cc.draw();
		c.move(x, y);
		
	}

}
